{
  "name": "fullstack-app-template",
  "version": "0.0.1",
  "description": "Template for ES6 Single Page Apps with small Express backend",
  "main": "server.es6",
  "dependencies": {
    "babel": "^5.2.6",
    "compression": "~1.4.3",
    "express": "~4.12.3",
    "morgan": "~1.5.2",
    "express-handlebars": "^2.0.1"
  },
  "devDependencies": {
    "babelify": "~6.0.2",
    "browser-sync": "~2.6.9",
    "browserify": "~10.0.0",
    "del": "~1.1.1",
    "gulp": "~3.8.11",
    "gulp-nodemon": "~2.0.3",
    "gulp-plumber": "^1.0.0",
    "gulp-sass": "~1.3.3",
    "gulp-tap": "^0.1.3",
    "jasmine": "^2.3.1",
    "portfinder": "~0.4.0",
    "supertest": "^0.15.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.4"
  },
  "scripts": {
    "start": "node_modules/.bin/babel-node ./server.es6",
    "serve": "node_modules/.bin/gulp serve",
    "build": "node_modules/.bin/gulp build",
    "postinstall": "test \"$NODE_ENV\" = \"production\" && (npm run build || exit 1) || exit 0",
    "test": "node_modules/.bin/babel-node node_modules/jasmine/bin/jasmine.js"
  },
  "keywords": [
    "gulp",
    "express",
    "babel",
    "scss",
    "es6",
    "jasmine",
    "browserify",
    "boilerplate",
    "template"
  ],
  "author": "Tomasz Szczęśniak-Szlagowski <spect88@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "0.10.x",
    "npm": "^2.2.0"
  },
  "private": true
}
